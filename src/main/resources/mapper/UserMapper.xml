<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.achan.dao.UserDao" >

    <select id="selectByPhone" resultType="com.achan.entity.UserVo">
        select * from user
        where phone = #{phone}
    </select>
</mapper>